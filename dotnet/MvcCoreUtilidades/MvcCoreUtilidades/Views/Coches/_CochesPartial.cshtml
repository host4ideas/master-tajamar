@model List<Coche>

<h1>Coches Partial</h1>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Marca</th>
            <th>Modelo</th>
            <th>Imagen</th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach (Coche car in Model)
            {
                <tr>
                    <td>@car.IdCoche</td>
                    <td>@car.Marca</td>
                    <td>@car.Modelo</td>
                    <td><img src="@car.Imagen" width="150" /></td>
                </tr>
            }
        }
    </tbody>
</table>
