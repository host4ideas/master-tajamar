@{
    if (Context?.User?.Identity?.IsAuthenticated == true)
    {
        var user = Context?.User?.Identity;

        <li class="nav-item">
            <a class="nav-link text-dark" asp-area="" asp-controller="Managed" asp-action="Logout">Logout</a>
        </li>
        <li class="nav-item">
            <a class="nav-link text-dark" asp-area="" asp-controller="Usuarios" asp-action="Perfil">@user.Name</a>
        </li>
    }
    else
    {
        <li class="nav-item">
            <a class="nav-link text-dark" asp-area="" asp-controller="Usuarios" asp-action="Perfil">Login</a>
        </li>
    }
}
