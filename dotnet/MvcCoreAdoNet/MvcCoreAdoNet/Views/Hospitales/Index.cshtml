@model List<Hospital>

<h1 style="color:blue">VISTA HOSPITALES</h1>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>NOMBRE</th>
            <th>TELEFONO</th>
            <th>CAMAS</th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach (var hospital in Model)
            {
                <tr>
                    <td>@hospital.IdHospital</td>
                    <td>@hospital.HospitalName</td>
                    <td>@hospital.Telefono</td>
                    <td>@hospital.Camas</td>
                </tr>
            }
        }
    </tbody>
</table>