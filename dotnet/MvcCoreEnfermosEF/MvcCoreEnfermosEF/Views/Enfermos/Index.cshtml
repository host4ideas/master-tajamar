@model List<Enfermo>

<table class="table">
    <thead>
        <tr>
            <th>Nº Inscripcion</th>
            <th>Apellido</th>
            <th>Direccion</th>
            <th>Fecha de nacimiento</th>
            <th>Sexo</th>
            <th>Nº Seguridad Social</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach (Enfermo enfermo in Model)
            {
                <tr>
                    <td>@enfermo.Inscripcion</td>
                    <td>@enfermo.Apellido</td>
                    <td>@enfermo.Direccion</td>
                    <td>@enfermo.FechaNac</td>
                    <td>@enfermo.Sexo</td>
                    <td>@enfermo.NumeroSS</td>
                    <td><a asp-controller="Enfermos" asp-action="Detalles" asp-route-inscripcion="@enfermo.Inscripcion" class="btn btn-outline-warning">👁️‍🗨️</a></td>
                </tr>
            }
        }
    </tbody>
</table>