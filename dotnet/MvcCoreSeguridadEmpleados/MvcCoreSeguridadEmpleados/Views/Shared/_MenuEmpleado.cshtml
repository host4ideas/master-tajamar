@{
    if (Context.User.Identity.IsAuthenticated == false)
    {
        <li class="nav-item">
            <a asp-controller="Empleados" asp-action="PerfilEmpleado" class="nav-link">
                Login
            </a>
        </li>
    }
    else
    {
        <li class="nav-item">
            <a asp-controller="Managed" asp-action="Logout" class="nav-link">
                Logout @Context.User.Identity.Name
            </a>
        </li>
    }
}
