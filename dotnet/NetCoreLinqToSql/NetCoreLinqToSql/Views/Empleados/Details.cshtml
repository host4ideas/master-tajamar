@model Empleado

<h1 style="color:red">Details</h1>

<a asp-controller="Empleados" asp-action="Index" class="link link-primary">Volver</a>

<hr />

<ul class="list-group">
    <li class="list-group-item">
        Apellido: @Model.Apellido
    </li>
    <li class="list-group-item">
        Oficio: @Model.Oficio
    </li>
    <li class="list-group-item">
        Salario: @Model.Salario
    </li>
</ul>