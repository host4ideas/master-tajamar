@model string[]

<h1 style="color: blue;">Pedido realizado correctamente</h1>

<h1 style="color: red;">Dirección de envío: @ViewData["DIRECCION"]</h1>

<ul class="list-group">
    @{
        foreach (string producto in Model)
        {
            <li class="list-group-item">@producto</li>
        }
    }
</ul>
