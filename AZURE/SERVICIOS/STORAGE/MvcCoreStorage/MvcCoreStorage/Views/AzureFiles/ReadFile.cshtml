<h1>Read File</h1>

<a asp-controller="AzureFiles" asp-action="Index">
    Volver a index
</a>

@if (ViewData["CONTENT_TYPE"].ToString().StartsWith("image"))
{
    <img width="300" src="@ViewData["CONTENT"]" />
}
else
{
    <p style="color: blueviolet; font-size:large;">
        @ViewData["CONTENT"]
    </p>
}
