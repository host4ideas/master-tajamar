@model Empleado

<a asp-controller="Empleados" asp-action="EmpleadosOficio">Volver</a>

<ul class="list-group">
    <li class="list-group-item">
        Apellido: @Model.Apellido
    </li>
    <li class="list-group-item">
        Oficio: @Model.Oficio
    </li>
    <li class="list-group-item">
        Salario: @Model.Salario
    </li>
    <li class="list-group-item">
        ID Dept: @Model.IdDepartamento
    </li>
</ul>
