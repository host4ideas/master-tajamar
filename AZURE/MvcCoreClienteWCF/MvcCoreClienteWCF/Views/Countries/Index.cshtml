@model tCountryCodeAndName[]

<ul class="list-group">
    @{
        foreach (tCountryCodeAndName country in Model)
        {
            <li class="list-group-item">
                @country.sName
                <a asp-controller="Countries" asp-action="Details" asp-route-isoCode="@country.sISOCode">Details</a>
            </li>
        }
    }
</ul>
